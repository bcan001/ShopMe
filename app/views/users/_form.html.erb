<%= form_for @user do |f| %>  
	<p>First Name</p>
  <p><%= f.text_field :first_name, :size => 40 %></p>
	<p>Last Name</p>
  <p><%= f.text_field :last_name, :size => 40 %></p>
	<p>Email</p>
  <p><%= f.text_field :email, :size => 40 %></p>
	<p>Username</p>
  <p><%= f.text_field :username, :size => 40 %></p>
 	<p>Password</p>
  <p><%= f.password_field :password, :size => 40 %></p>
  <p><%= f.submit "Sign Up" %></p>
<% end %>

