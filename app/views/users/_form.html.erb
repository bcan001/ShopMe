<%= form_for @user do |f| %>  
	<p>First Name</p>
  <p><%= f.text_field :first_name, :size => 40, placeholder: "First Name" %></p>
	<p>Last Name</p>
  <p><%= f.text_field :last_name, :size => 40, placeholder: "Last Name" %></p>
	<p>Email</p>
  <p><%= f.text_field :email, :size => 40, placeholder: "Email" %></p>
	<p>Username</p>
  <p><%= f.text_field :username, :size => 40, placeholder: "Username" %></p>
 	<p>Password</p>
  <p><%= f.password_field :password, :size => 40, placeholder: "Password" %></p>
  <p><%= f.submit "Submit", class: "btn btn-primary"%></p>
<% end %>

